	SETZM	17

	WRUBR	MTBLK1
	MOVSI	17,-17
	SETZM	(17)		;ZERO AC'S
	AOBJN	17,.-1
	SETZM	17

	WRUBR	MTBLK0
	MOVSI	17,-17
	SETZM	(17)		;ZERO AC'S
	AOBJN	17,.-1
	SETZM	17

	JRST	@20		;START BOOT


