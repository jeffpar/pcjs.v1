DEFINE	CMTAB(CMD,ADR),<
	[ASCIZ "CMD"],,ADR
>

DEFINE	CMABR(ABBR,REST,ADDR),<
	[ASCIZ "ABBR"],,[ TLNN T1,(CM%ESC)
			  JRST ADDR
			  HRROI	T1,[ASCIZ "REST "]
			  SKIPN MONTYP
				JRST [HRRZ 1,1
				OUTSTR @1
				JRST ADDR]
			  PSOUT
			  JRST ADDR]
>

DEFINE	CMD(FUNC,ERR,%A),<
%A:!	XLIST
IFNB <ERR>,<
	MOVEI	T1,[ASCIZ "ERR"]
	MOVEM	T1,CMDMSG
>
IFB <ERR>,<
	SETZM	CMDMSG
>
	MOVEI	T1,CSB
	MOVEI	T2,FUNC
	SKIPN	MONTYP
	JRST	[GO 	S%CMND
		 JRST	.+3]
	COMND
	ERJMP	[PUSH P,[%A]
		 JRST CMJERR]
	TLNE	T1,(CM%NOP)
	GO	CMDERR
	LIST
>
DEFINE	NOISE(TEXT),<
	CMD	[FLDDB.(.CMNOI,,<-1,,[ASCIZ "TEXT"]>)],<EXPECTING TEXT>
>
DEFINE	CMDNOP(FUNC,ERR,%A),<
%A:!	XLIST
IFNB <ERR>,<
	MOVEI	T1,[ASCIZ "ERR"]
	MOVEM	T1,CMDMSG
>
IFB <ERR>,<
	SETZM	CMDMSG
>
	MOVEI	T1,CSB
	MOVEI	T2,FUNC
	SKIPN	MONTYP
	JRST	[GO 	S%CMND
		 JRST	.+3]
	COMND
	ERJMP	[PUSH P,[%A]
		 JRST CMJERR]
	TLNE	T1,(CM%NOP)
	LIST
>

DEFINE	ERR(TEXT),<
	GO	[HRROI	T1,[ASCIZ "TEXT"]
		 JRST	ERR%]
>

